<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:context = "http://www.springframework.org/schema/context"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans     
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context 
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 
 <context:annotation-config  />  
   
<bean class="com.bhatiya.autowire.annotation.Address" name="ad">
<property name="city" value="pune" />
<property name="state" value="bihar" />
</bean>

<bean class="com.bhatiya.autowire.annotation.Address" name="ad2">
<property name="city" value="ranchi" />
<property name="state" value="jharkhand" />
</bean>

<bean name="st" class="com.bhatiya.autowire.annotation.Student" ></bean>

 </beans>